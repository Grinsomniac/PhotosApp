<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.photos.controllers.TagController">
   <children>
      <TextField fx:id="createTagName" layoutX="20.0" layoutY="54.0" promptText="Tag" />
      <TextField fx:id="createTagValue" layoutX="20.0" layoutY="106.0" promptText="Value" />
      <Button fx:id="cancelCreateTag" layoutX="30.0" layoutY="147.0" mnemonicParsing="false" onAction="#handleCancelTagCreate" text="Back" />
      <Button fx:id="confirmTagCreate" layoutX="110.0" layoutY="147.0" mnemonicParsing="false" onAction="#handleTagConfirm" text="Confirm" />
      <Label alignment="CENTER" layoutX="44.0" layoutY="14.0" text="Tag Editor" textAlignment="CENTER">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
