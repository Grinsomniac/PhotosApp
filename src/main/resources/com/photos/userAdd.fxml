<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="170.0" prefWidth="264.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.photos.controllers.AddUserController">
   <children>
      <TextField fx:id="createUserText" layoutX="58.0" layoutY="73.0" promptText="Enter username" />
      <Button fx:id="cancelUserCreate" layoutX="32.0" layoutY="120.0" mnemonicParsing="false" onAction="#handleCancelUserCreate" text="Cancel" />
      <Button fx:id="createUser" layoutX="182.0" layoutY="120.0" mnemonicParsing="false" onAction="#handleUserCreate" text="Create" />
      <Label layoutX="58.0" layoutY="23.0" text="Create User">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
