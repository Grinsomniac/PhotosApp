<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="219.0" prefWidth="279.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.photos.controllers.CopyController">
   <children>
      <Button fx:id="copyConfirm" layoutX="136.0" layoutY="149.0" mnemonicParsing="false" onAction="#handleCopyConfirm" text="Copy to Album">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="cancelCopy" layoutX="25.0" layoutY="149.0" mnemonicParsing="false" onAction="#handleCancelCopy" text="Cancel">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <ComboBox fx:id="albumCopyList" layoutX="59.0" layoutY="94.0" prefWidth="150.0" promptText="Select Album" />
      <Label layoutX="69.0" layoutY="31.0" text="Copy Photo">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
